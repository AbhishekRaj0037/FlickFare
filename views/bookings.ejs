<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile Page</title>

    <!-- Custom Css -->
    <link rel="stylesheet" href="style.css">

    <!-- FontAwesome 5 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css">
</head>
<body>
    <!-- Navbar top -->
    

   
    </div>
    <!-- End -->

    <!-- Sidenav -->
    <div class="sidenav">
        <div class="profile">
            <img src=<%=user.image%> alt="" width="200" height="200">

            
        </div>

        <div class="sidenav-url">
            <div class="url">
                <a href="/profile" class="active">Profile</a>
                <a href="/bookings" class="active">Bookings</a>
                <a href="/changepassword" class="active">Change Password</a>
                <hr align="center">
            </div>
        
        </div>
    </div>
    <!-- End -->

    <!-- Main -->
    <div class="main">
        <h2>BOOKINGS</h2>
        <div class="card">
            <div class="card-body">
            
                <main role="main">
                    <div class="box-wrap">
                      <div class="table-wrap">
                        <table>
                          <thead>
                            <tr>
                              <th>Show Name</th>
                              <th>Seat Number</th>
                              <th>Date of booking</th>
                              <th>Invoice</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <% for (let currentBooking of booking) {%>
                            <tr>
                              
                              <td><%=currentBooking.showName%></td>
                              <td><%=currentBooking.seat%></td>
                              <td><%=currentBooking.date%></td>
                              
                              <td><a href=<%=currentBooking.invoice%> target=<%=currentBooking.invoice%>>Invoice</a></td>
                              <td style="color: rgb(254, 0, 0)">
                                <a href="/booking/delete/<%=currentBooking._id%>">
                                  Cancel Booking</a
                                >
                              </td>
                            </tr>
                            <%}%>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </main>
            </div>
        </div>

        
    </div>
